<%= simple_form_for(@course) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :user_id ,label:'Instrutor'%>
    <%= f.input :limit %>
    <%= f.input :fee %>
    <%= f.input :start %>
    <%= f.input :end %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
